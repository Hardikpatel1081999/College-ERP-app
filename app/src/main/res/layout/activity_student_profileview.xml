<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    tools:context=".student_profileview">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/tvimg"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        android:layout_marginTop="55dp"
     android:layout_marginBottom="10dp"
        android:padding="08dp"
        android:elevation="6dp"
        />

    <TextView
        android:id="@+id/tvemail"
        android:layout_width="237dp"
        android:layout_height="38dp"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="Name :" />

        <TextView
            android:id="@+id/tvname"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:textAllCaps="true"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Application No :" />

        <TextView
            android:id="@+id/tvappno"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="Date of Admission:" />

        <TextView
            android:id="@+id/tvdoa"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="Present Address :" />

        <TextView
            android:id="@+id/tvaddress"
            android:layout_width="269dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/thin"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="Zip :" />

        <TextView
            android:id="@+id/tvzip"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="State :" />

        <TextView
            android:id="@+id/tvstate"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:text="Date Of Birth :" />

        <TextView
            android:id="@+id/tvdob"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Gender :" />

        <TextView
            android:id="@+id/tvgender"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:text="Nationality :" />

        <TextView
            android:id="@+id/tvnation"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Contact No :" />

        <TextView
            android:id="@+id/tvphone"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Emergancy Contanct No:" />

        <TextView
            android:id="@+id/tvephone"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Branch :" />

        <TextView
            android:id="@+id/tvbranch"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text="" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="19dp"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:layout_marginRight="10dp"
            android:text="Class :" />

        <TextView
            android:id="@+id/tvclass"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="#303561"
            android:textSize="20dp"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="50dp"
            android:layout_weight="1"
            android:fontFamily="@font/thin"
            android:gravity="center"
            android:text="Semester :"
            android:textColor="#303561"
            android:textSize="19dp" />

        <TextView
            android:id="@+id/tvsem"
            android:layout_width="269dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="50dp"
            android:layout_weight="1"
            android:fontFamily="@font/thin"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="#303561"
            android:textSize="20dp" />
    </LinearLayout>


</LinearLayout>
</ScrollView>