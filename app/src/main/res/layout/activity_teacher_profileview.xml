<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    tools:context=".Teacher_profileview">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/tvt_image"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="55dp"
            android:layout_marginBottom="10dp"
            android:padding="08dp"
            android:elevation="6dp"
            />

        <TextView
            android:id="@+id/tvt_email"
            android:layout_width="237dp"
            android:layout_height="38dp"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:text="" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:layout_marginRight="10dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:text="Name :" />

            <TextView
                android:id="@+id/tvt_name"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:textAllCaps="true"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:text="Application No :" />

            <TextView
                android:id="@+id/tvt_appno"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:layout_marginRight="10dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:text="Date of Joining:" />

            <TextView
                android:id="@+id/tvt_doa"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:text="Present Address :" />

            <TextView
                android:id="@+id/tvt_address"
                android:layout_width="269dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:fontFamily="@font/thin"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:layout_marginRight="10dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:text="Zip :" />

            <TextView
                android:id="@+id/tvt_zip"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:layout_marginRight="10dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:text="State :" />

            <TextView
                android:id="@+id/tvt_state"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:layout_marginRight="10dp"
                android:gravity="center"
                android:text="Date Of Birth :" />

            <TextView
                android:id="@+id/tvt_dob"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:text="Gender :" />

            <TextView
                android:id="@+id/tvt_gender"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:layout_marginRight="10dp"
                android:gravity="center"
                android:text="Nationality :" />

            <TextView
                android:id="@+id/tvt_nation"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:text="Contact No :" />

            <TextView
                android:id="@+id/tvt_phone"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:text="Emergancy Contanct No:" />

            <TextView
                android:id="@+id/tvt_ephone"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:text="Branch :" />

            <TextView
                android:id="@+id/tvt_branch"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="152dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="19dp"
                android:fontFamily="@font/thin"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="50dp"
                android:text="Level Of Education :" />

            <TextView
                android:id="@+id/tvt_education"
                android:layout_width="269dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="#303561"
                android:textSize="20dp"
                android:layout_marginBottom="50dp"
                android:fontFamily="@font/thin"
                android:gravity="center_vertical"
                android:text=""/>
        </LinearLayout>


    </LinearLayout>
</ScrollView>